using namespace std;

// }}}

class AttackOfTheClones {
 public:
  int count(vector <int> firstWeek, vector <int> lastWeek) {
    const int N = firstWeek.size();
    auto begins = firstWeek;
    int days = 1;
    while (true) {
      // test stop condition.
      bool stop = true;
      for (int i = 0; i < N; ++i) {
        if (begins[i] > lastWeek[i]) {
          stop = false;
          break;
        }
      }
      if (stop) {
        break;
      }

      // update begins.
      for (int i = 0; i < N; ++i) {
        begins[i] = max(0, begins[i] - 1);
      }
      ++days;
    }
    return days;
  }
};